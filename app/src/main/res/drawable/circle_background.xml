<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- Màu nền cho hình tròn -->
    <solid android:color="#CCCCCC" />

    <!-- Đường viền của hình tròn (tuỳ chọn) -->
    <stroke
        android:width="1dp"
        android:color="#FFFFFF" />

    <!-- Kích thước tối thiểu cho hình tròn -->
    <size
        android:width="50dp"
        android:height="50dp" />

</shape>
